{
  "name": "virtuoso-app",
  "author": "Akshaj Jagarapu",
  "private": true,
  "description": "Free JavaScript classes brimming with critical thinking activities, enriching code labs, inspiring projects, and more. Learn quintessential skills required for competing in this age of technology.",
  "version": "0.0.0",
  "scripts": {
    "start": "npm run reset && npm run format && gatsby build && cp -R public app.govirtuoso.org && gatsby serve",
    "test": "npm run format && gatsby develop",
    "reset": "gatsby clean && rm -rf govirtuoso.org",
    "format": "prettier --write \"**/*.{js,jsx,scss,json}\""
  },
  "dependencies": {
    "@reach/router": "^1.3.4",
    "bootstrap": "^5.0.0",
    "gatsby": "^3.5.0",
    "gatsby-plugin-image": "^1.5.0",
    "gatsby-plugin-manifest": "^3.5.0",
    "gatsby-plugin-offline": "^4.5.0",
    "gatsby-plugin-preact": "^5.5.0",
    "gatsby-plugin-purgecss": "^6.0.1",
    "gatsby-plugin-react-helmet": "^4.5.0",
    "gatsby-plugin-sass": "^4.5.0",
    "gatsby-plugin-sharp": "^3.5.0",
    "gatsby-plugin-sitemap": "^4.1.0",
    "gatsby-source-filesystem": "^3.5.0",
    "preact": "^10.5.13",
    "preact-render-to-string": "^5.1.19",
    "react-helmet": "^6.1.0",
    "react-jutsu": "^3.0.2",
    "sass": "^1.32.12"
  },
  "devDependencies": {
    "prettier": "^2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AskJag07/virtuoso-app"
  }
}
